@model IEnumerable<DMS_WEB.Models.GET_ACCOUNT_DOC_LIST_FOR_REQUISITION_MISMATCH_CHECK_Result>

@{
    Layout = null;
    ViewBag.Title = "Checking Account Document List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

<div class="div_row">
 @*   <div class="div_cell div_caption">Select</div>
    <div class="div_cell div_caption"></div>
    <div class="div_cell div_caption">Client</div>
    <div class="div_cell div_caption">Department</div>*@
    <div class="div_cell div_caption">Account ID</div>
    <div class="div_cell div_caption">Document Type</div>
    <div class="div_cell div_caption">Status</div>
    @*<div class="div_cell div_caption">Delete</div>*@
</div>

@foreach (var item in Model) {
    
    <div class="div_row">
        <div class="div_cell">@Html.DisplayFor(modelItem => item.ACC_ID)</div>
        <div class="div_cell">@Html.DisplayFor(modelItem => item.DOC_TYPE)</div>
        <div class="div_cell">@Html.DisplayFor(modelItem => item.TRN_TYPE_NAME)</div>
@*        <div class="div_cell">@Html.DisplayFor(modelItem => item.RECALL_BOX_ID)</div>
        <div class="div_cell">@Html.DisplayFor(modelItem => item.RECALL_LOCATION)</div>*@
    </div>    

}

<div class="div_row">
    <div class="div_cell div_caption">
        @Html.ActionLink("Save", "SaveList", "Requisition", new { @class = "btn btn-success" })   
    </div>
     <div class="div_cell div_caption">
        @Html.ActionLink("Cancel", "Index", "Requisition", new { @class = "confirm btn btn-danger delete" })   
    </div>
</div>
