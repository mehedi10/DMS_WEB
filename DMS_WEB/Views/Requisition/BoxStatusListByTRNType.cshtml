
@using DMS_WEB.Models
@model IEnumerable<DMS_WEB.Models.GET_BOX_COUNT_FOR_REQUISITION_MISMATCH_CHECK_Result>

@{
    ViewBag.Title = "Checking Box List";
}

<h2>@ViewBag.Title</h2>

<div class="div_row tab_header">
    <div class="div_cell div_caption">Status</div>
    <div class="div_cell div_caption">Total</div>
</div>
@foreach (var item in Session["list_For_Box_Requisition"] as List<GET_BOX_COUNT_FOR_REQUISITION_MISMATCH_CHECK_Result>)
{
    string str_tran_type = string.Empty;
  
    if (item.TRN_TYPE_NO == 1)
    {
        str_tran_type = "IN";
    } 
    else if (item.TRN_TYPE_NO == 2)
    {
        str_tran_type = "OUT";
    }
    else
    {
        str_tran_type = "Not Recieved";
    }
    
    <div class="div_row">
        <div class="div_cell div_caption">@str_tran_type</div>
        <div class="div_cell">
                @Html.ActionLink(item.TotalNo.ToString(), "ShowManualDetails", "Requisition", new { @TRN_TYPE_NO = item.TRN_TYPE_NO }, new { @class = "btn btn-primary" })
        </div>                       
    </div>    
}

<div class="div_row">
    <div class="div_cell div_caption">
        @Html.ActionLink("Save", "SaveList", "Requisition", new { @class = "btn btn-success" })   
    </div>
     <div class="div_cell div_caption">
        @Html.ActionLink("Cancel", "Index", "Requisition", new { @class = "confirm btn btn-danger delete" })   
    </div>
</div>
